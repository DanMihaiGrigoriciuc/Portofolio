{% set navPages = collections.all | eleventyNavigation %}
<nav class="dg-nav">
  <ul role="list" >
    {%- for entry in navPages %}
      <li>
        <a class="dg-button" href="{{ entry.url | url }}" {% if entry.url == page.url %} aria-current="page" {% endif %}>{{ entry.title }}</a>
      </li>
    {%- endfor %}
  </ul>
</nav>